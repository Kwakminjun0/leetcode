    "class Solution(object):\n",
    "    def trailingZeroes(self, n):\n",
    "        \"\"\"\n",
    "        :type n: int\n",
    "        :rtype: int\n",
    "        \"\"\"\n",
    "        count = 0\n",
    "        while n >= 5:\n",
    "            count += n // 5\n",
    "            n //= 5\n",
    "        return count\n",
    "        "
   ]
