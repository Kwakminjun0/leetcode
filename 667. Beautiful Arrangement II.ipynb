    "class Solution(object):\n",
    "    def constructArray(self, n, k):\n",
    "        \"\"\"\n",
    "        :type n: int\n",
    "        :type k: int\n",
    "        :rtype: List[int]\n",
    "        \"\"\"\n",
    "        answer = list(range(1, n + 1))\n",
    "        for i in range(1, k):\n",
    "            answer[i:] = answer[:i-1:-1]\n",
    "        return answer"
   ]
