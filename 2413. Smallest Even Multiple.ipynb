    "class Solution(object):\n",
    "    def smallestEvenMultiple(self, n):\n",
    "        \"\"\"\n",
    "        :type n: int\n",
    "        :rtype: int\n",
    "        \"\"\"\n",
    "        if n % 2 == 0:\n",
    "            return n\n",
    "        else:\n",
    "            return 2 * n"
