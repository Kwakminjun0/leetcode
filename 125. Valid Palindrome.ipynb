   "source": [
    "class Solution(object):\n",
    "    def isPalindrome(self, s):\n",
    "\n",
    "        s = s.lower()\n",
    "        a = ''.join(char for char in s if char.isalnum())\n",
    "        return a == a[::-1]"
   ]
  }
