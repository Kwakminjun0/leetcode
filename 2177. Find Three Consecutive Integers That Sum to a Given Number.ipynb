    "class Solution(object):\n",
    "    def sumOfThree(self, num):\n",
    "        \"\"\"\n",
    "        :type num: int\n",
    "        :rtype: List[int]\n",
    "        \"\"\"\n",
    "        val=num//3\n",
    "        while val>=0 and 3*val==num:\n",
    "            return [val-1,val,val+1]\n",
    "        return []"
   ]
