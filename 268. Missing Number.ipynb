    "    def missingNumber(self, nums):\n",
    "        \"\"\"\n",
    "        :type nums: List[int]\n",
    "        :rtype: int\n",
    "        \"\"\"\n",
    "        n = len(nums)\n",
    "        total_sum = n * (n + 1) // 2\n",
    "        actual_sum = sum(nums)\n",
    "        return total_sum - actual_sum\n",
